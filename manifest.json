{
  "manifest_version": 2,
  "name": "水洞",
  "version": "1.0",
  "description": "一款面向安服仔的碰瓷工具(<url id=\"cv2hjf4bcdrj2q93s550\" type=\"url\" status=\"parsed\" title=\"GitHub - yuning029/WaterHole: 一款面向安服仔的水洞插件\" wc=\"3117\">https://github.com/yuning029/WaterHole</url> )",
  "permissions": [
    "activeTab",
    "*://*/*",
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true // Change this to true
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/favicon-16x16.png",
      "32": "images/favicon-32x32.png",
      "192": "images/favicon-192x192.png"
    }
  },
  "icons": {
    "16": "images/favicon-16x16.png",
    "32": "images/favicon-32x32.png",
    "192": "images/favicon-192x192.png"
  },
  "web_accessible_resources": [
    "Clickjacking_PoC.html",
    "images/*.png"
  ]
}
